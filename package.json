{
  "name": "manifold-docs",
  "version": "1.0.0",
  "description": "Manifold Jekyll Docs",
  "main": "manifest.js",
  "repository": "https://github.com/manifoldScholar/manifold-docs",
  "author": "Cast Iron Coding",
  "license": "MIT",
  "scripts": {
    "clean-build": "rimraf docs-assets",
    "dev": "NODE_ENV=development WEBPACK_DEV_SERVER=true webpack-dev-server --config webpack/base.conf.js",
    "build": "NODE_ENV=production webpack --bail --config webpack/base.conf.js",
    "pretty": "prettier --write \"{themes,webpack}/**/*.js\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "donezo": "castiron/donezo",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "lint-staged": "^4.0.3",
    "lodash": "^4.17.19",
    "node-sass": "^4.13.1",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "prettier": "^1.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-recess-order": "^1.2.2",
    "stylelint-scss": "^1.5.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglify-js": "^2.8.29",
    "uglify-loader": "^2.0.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^3.1.11",
    "webpack-manifest-plugin": "^1.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "broken-link-checker": "^0.7.8"
  }
}
